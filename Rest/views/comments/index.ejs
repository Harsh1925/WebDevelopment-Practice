<!DOCTYPE html>
<html lang="en">


<%- include("partials/head")%>

<body>
    <%- include("partials/navbar")%>
    <div class="container">
        <h1>Comments</h1>
        <ul>
            <% for(let com of comments){ %>
            <li><%= com.comment %> - <b><%= com.username %></b>
                <a href="/comments/<%= com.id %>">Link</a>
            </li>
            <% } %>
        </ul>
    </div>
</body>

</html>